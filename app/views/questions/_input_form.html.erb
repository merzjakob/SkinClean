<%= simple_form_for [question, @patient_answer], html: {id: "chatForm"} do |f| %>
  <% if question.photo %>
      <%= f.input :photo %>
      <%= f.input :photo_cache, as: :hidden %>
  <% else %>
      <%= f.input :content, label: false, wrapper_html: {id: "inputchat"} %>
  <% end %>
  <%= f.submit "Send", id: "submitchat", class: "btn-solid" %>
<% end %>
